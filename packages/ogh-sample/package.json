    "@yamadayuki/ogh": "^1.1.0",
    "execa": "^4.0.3",
    "prettier": "^2.0.5"
  },
  "files": [
    "lib",
    ".prettierrc"
  ],
  "publishConfig": {
    "access": "public"
  }
}
